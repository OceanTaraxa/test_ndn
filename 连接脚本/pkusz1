#!/usr/bin/expect
set timeout 2
set password password
spawn ssh pkusz1@ft.qjm253.cn -p 9604

expect {
    "(yes/no)?" {
        send "yes\r";
        expect "password:";
        send "${password}\r";
        exp_continue;
    }
    "password:" {
        send "${password}\r";
        exp_continue;
    }
}
interact
